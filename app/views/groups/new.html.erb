<%= form_tag("/groups", method:"POST") do %>
  <%= label_tag :cohort, "Select Cohort" %>
  <%= select_tag "cohort", options_from_collection_for_select(@cohorts, "id", "name") %></br>
  <%= label_tag :number, "Select GroupType" %>
  <%= select_tag "group_types", options_from_collection_for_select(@group_types, "id", "name") %></br>
  <%= submit_tag "Submit" %>
<% end %>
